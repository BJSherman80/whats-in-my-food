<h1> Food Search Results </h1>
<% @ten_foods.each do |food| %>
  <ul class="food">
    <li class="code">Code: <%= food[:fdcId] %></li>
    <li class="description">Description: <%= food[:description] %></li>
    <li class="brand_owner">Owner: <%= food[:commonNames] %></li>
    <li class="ingredients">Nutrients:
      <% food[:foodNutrients].each do |nutrient|%>
      <%= nutrient[:nutrientName] %></li>
    <% end %>
  </ul>
  <% end %>
